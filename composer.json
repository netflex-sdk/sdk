{
  "name": "netflex/sdk",
  "description": "Netflex platform SDK",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "sdk",
    "netflex",
    "framework"
  ],
  "homepage": "https://github.com/netflex-sdk/sdk",
  "authors": [
    {
      "name": "Thomas Alrek",
      "email": "thomas@apility.no",
      "homepage": "https://github.com/thomas-alrek",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/netflex-sdk/sdk/issues",
    "source": "https://github.com/netflex-sdk/sdk"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Netflex\\": "src/"
    }
  },
  "config": {
    "platform": {
      "php": "7.3"
    },
    "sort-packages": true
  },
  "require": {
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^6.9",
    "laravel/tinker": "^2.0",
    "netflex/customers": "^1.0.0-alpha",
    "netflex/support": "^2.0",
    "php": "^7.3",
    "symfony/psr-http-message-bridge": "^2.0"
  },
  "require-dev": {
    "symfony/cache": "^5.0"
  },
  "suggest": {
    "apility/attribute-list": "Helper class for generating attribute lists for Vue components",
    "apility/opengraph": "Helper library for creating OpenGraph meta tags",
    "apility/netflex-commerce": "Makes it easier to work with the Netflex Commerce API"
  }
}
